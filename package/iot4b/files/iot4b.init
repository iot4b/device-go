#!/bin/sh /etc/rc.common

START=99
USE_PROCD=1

SERVICE_DAEMONIZE=1
SERVICE_WRITE_PID=1

SERVICE_NAME="iot4b"

start_service() {
    config_load iot4b
    config_get enabled iot4b enabled 0
    [ "$enabled" = "1" ] || return 0

    config_get param iot4b param 'default'
    procd_open_instance
    procd_set_param command /usr/bin/iot4b -env=iot4b
    procd_close_instance
}